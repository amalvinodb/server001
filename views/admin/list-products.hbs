<section>
    <div class="container">
        <table class="table">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">name</th>
      <th scope="col">catagory</th>
      <th scope="col">discription</th>
      <th scope="col">image</th>
    </tr>
  </thead>
  <tbody>
    {{#each product}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.catagory}}</td>
      <td>{{this.discription}}</td>
      <td> <img src="{{this.image}}" alt=""> </td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>